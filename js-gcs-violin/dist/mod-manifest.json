{
    "apiVersion": "1.1",
    "version": "1.0",
    "name": "Violin chart",
    "id": "genware-violin-chart-mod",
    "icon": "icon.svg",
    "properties": [
        {
            "name": "flipAxis",
            "type": "boolean",
            "defaultValue": "false"
        },
        {
            "name": "colorforViolin",
            "type": "boolean",
            "defaultValue": "false"
        },
        {
            "name": "includeBoxplot",
            "type": "boolean",
            "defaultValue": "false"
        },
        {
            "name": "resolution",
            "type": "double",
            "defaultValue": "0.2"
        },
        {
            "name": "violintype",
            "type": "string",
            "defaultValue": "basic"
        }
    ],
    "dataViewDefinition": {
        "colorAxis": {
            "mode": "categorical",
            "dropTarget": {
                "icon": "Color",
                "description": "Color by {0}"
            },
            "allowNonAggregatingMeasures":true
        },
        "axes": [
            {
                "name": "X",
                "mode": "categorical",
                "placement": "bottom",
                "dropTarget": {
                    "icon": "XAxis3D",
                    "description": "Use {0} on the mod's X-axis"
                }
            },
            {
                "name": "Row ID",
                "mode": "categorical",
                "placement": "none"
                
            },
            {
                "name": "Y",
                "mode": "continuous",
                "placement": "left",
                "allowNonAggregatingMeasures": true
            }
        ]
    },
    "files": [
        "index.html",
        "main.css",
        "bundle.js"
    ]
}